<h1>Listing tests</h1>

<table>
  <thead>
    <tr>
      <th>Score</th>
      <th>Wpm</th>
      <th>Retention</th>
      <th>Overall</th>
      <th>User</th>
      <th>Campaign</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.score %></td>
        <td><%= test.WPM %></td>
        <td><%= test.retention %></td>
        <td><%= test.overall %></td>
        <td><%= test.user %></td>
        <td><%= test.campaign %></td>
        <td><%= link_to 'Show', test %></td>
        <td><%= link_to 'Edit', edit_test_path(test) %></td>
        <td><%= link_to 'Destroy', test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test', new_test_path %>
